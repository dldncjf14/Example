package com.spring.ex.dto;

public class PDto {
	private int num;
	private String id;
	private String name;
	private String age;
	
	public PDto(int number, String id, String name, String age) {
		super();
		this.num = number;
		this.id = id;
		this.name = name;
		this.age = age;
	}

	public int getNumber() {
		return num;
	}

	public void setNumber(int number) {
		this.num = number;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAge() {
		return age;
	}

	public void setAge(String age) {
		this.age = age;
	}

	public PDto() {
		super();
	}
	
	
}
